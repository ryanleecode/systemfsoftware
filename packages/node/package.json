{
  "name": "@systemfsoftware/node",
  "version": "0.4.1",
  "type": "module",
  "module": "./dist/esm/mod.js",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "@systemfsoftware/source": "./src/mod.ts",
        "types": "./dist/esm/mod.d.ts",
        "default": "./dist/esm/mod.js"
      }
    },
    "./Fetch": {
      "import": {
        "@systemfsoftware/source": "./src/Fetch.ts",
        "types": "./dist/esm/Fetch.d.ts",
        "default": "./dist/esm/Fetch.js"
      }
    }
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "tshy": {
    "project": "./tsconfig.build.json",
    "exports": {
      "./package.json": "./package.json",
      ".": "./src/mod.ts",
      "./Fetch": "./src/Fetch.ts"
    },
    "dialects": [
      "esm"
    ],
    "exclude": [
      "src/**/*.test.ts"
    ],
    "sourceDialects": [
      "@systemfsoftware/source"
    ]
  },
  "scripts": {
    "prepublishOnly": "nx build node"
  },
  "peerDependencies": {
    "undici": "^7.0.0"
  },
  "peerDependenciesMeta": {
    "undici": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/node": "^24.0.10",
    "typescript": "^5.8.3",
    "undici": "^7.11.0"
  }
}
